<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <customPermissions>
        <enabled>true</enabled>
        <name>LLC_BI__Exclude_Trigger</name>
    </customPermissions>
    <description>If a user has this permission set assigned, Apex code built with &quot;Exclude Trigger = False&quot; concepts will not evaluate. This allows users to bypass Apex automation when needed. Introduced Spring 2023.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Exclude Trigger</label>
</PermissionSet>