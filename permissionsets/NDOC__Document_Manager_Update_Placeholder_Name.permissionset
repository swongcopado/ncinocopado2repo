<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <customPermissions>
        <enabled>true</enabled>
        <name>NDOC__Document_Manager_Update_Placeholder_Name</name>
    </customPermissions>
    <description>Allows users to update a placeholder name. Without this permission, the user can only select the placeholder name when creating a new placeholder.</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Document Manager Update Placeholder Name</label>
</PermissionSet>