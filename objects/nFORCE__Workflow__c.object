    <fields>
        <fullName>nFORCE__Key__c</fullName>
        <caseSensitive>false</caseSensitive>
        <deprecated>false</deprecated>
        <description>The user manually populates this unique text field to provide an identifier for the system to locate the workflow. By default, this field is blank.</description>
        <externalId>false</externalId>
        <inlineHelpText>Enter a unique name in this field. Use this field to easily identify your workflow as you continue the workflow configuration process.</inlineHelpText>
        <label>Key</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>nFORCE__Orchestration_Type__c</fullName>
        <deprecated>false</deprecated>
        <description>The user manually populates this picklist field to indicate how the steps run in the workflow. Some possible options include Single Transaction, Multiple Transactions (Queued), and Multiple Transactions (UI Orchestrated).</description>
        <externalId>false</externalId>
        <inlineHelpText>Select the desired orchestration type for your workflow.</inlineHelpText>
        <label>Orchestration Type</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>nFORCE.WorkflowPerExeContextOrchestrator</fullName>
                    <default>false</default>
                    <label>Single Transaction</label>
                </value>
                <value>
                    <fullName>nFORCE.StepPerQueueJobOrchestrator</fullName>
                    <default>false</default>
                    <label>Multiple Transactions (Queued)</label>
                </value>
                <value>
                    <fullName>nFORCE.StepPerExeContextOrchestrator</fullName>
                    <default>false</default>
                    <label>Multiple Transactions (UI Orchestrated)</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <fields>
        <fullName>nFORCE__lookupKey__c</fullName>
        <caseSensitive>false</caseSensitive>
        <deprecated>false</deprecated>
        <description>The system automatically generates this number field with the External ID for the workflow object.</description>
        <externalId>true</externalId>
        <inlineHelpText>This field contains the External ID for the associated workflow.</inlineHelpText>
        <label>Lookup Key</label>
        <length>255</length>
